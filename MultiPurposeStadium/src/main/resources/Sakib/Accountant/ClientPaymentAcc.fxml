          <TableColumn fx:id="amountColCP" prefWidth="75.0" text="Amount" />
          <TableColumn fx:id="dateColCP" prefWidth="75.0" text="Date" />
            <TableColumn fx:id="methodColCP" prefWidth="75.0" text="Method" />
            <TableColumn fx:id="statusColCP" prefWidth="75.0" text="Status" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Text layoutX="190.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Client Payments">
