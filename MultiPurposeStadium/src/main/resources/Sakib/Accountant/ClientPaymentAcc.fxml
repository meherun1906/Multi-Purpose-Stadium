            <Font name="System Bold" size="25.0" />
         </font>
      </Text>
      <Button fx:id="confirmbutton" layoutX="32.0" layoutY="306.0" mnemonicParsing="false" onAction="#confirmButton" prefHeight="51.0" prefWidth="158.0" text="Confirm Payment Received" />
